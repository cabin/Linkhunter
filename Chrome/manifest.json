{
  "manifest_version": 2,
  "name": "Linkhunter",
  "version": "1.3.1",
  "default_locale": "en",
  "description": "Find your links on Delicious or Pinboard.",
  "homepage_url": "http://madebycabin.com/",
  "icons": {
    "128": "mark-128.png",
    "48": "mark-48.png",
    "16": "mark-16.png"
  },
  "background": {
    "scripts": ["b/background.js"]
  },
  "browser_action": {
    "default_icon": {
        "19": "browser-action.png",
        "38": "browser-action@2x.png"
    },
    "default_title": "Linkhunter",
    "default_popup": "b/linkhunter.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["b/content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "browser-action.png",
    "b/linkhunter.html",
    "iframe.css"
  ],
  "permissions": [
    "https://api.del.icio.us/*",
    "https://api.pinboard.in/*",
    "tabs"
  ]
}
